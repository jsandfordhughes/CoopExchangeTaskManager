{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-recommended",
        "plugin:prettier/recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "vue/multi-word-component-names": "off"
    },
    "globals": {
        "_": "readonly",
        "Ziggy": "readonly",
        "route": "readonly",
        "axios": "readonly",
        "Echo": "readonly"
    }
}
